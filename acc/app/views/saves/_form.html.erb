
<div class="row">
            <div class="box">
                <div class="col-lg-12 intro-text text-center">
			<%= form_for(@safe) do |f| %>
			  <% if @safe.errors.any? %>
			    <div id="error_explanation">
			      <h2><%= pluralize(@safe.errors.count, "error") %> prohibited this safe from being saved:</h2>

			      <ul>
			      <% @safe.errors.full_messages.each do |message| %>
				<li><%= message %></li>
			      <% end %>
			      </ul>
			    </div>
			  <% end %>

			  <div class="field">
			    <%= f.label :cash %><br>
			    <%= f.number_field :cash %>
			  </div>
			  <div class="field">
			    <%= f.label :date %><br>
			    <%= f.date_select :date %>
			  </div>
			  <div class="field">
			    <%= f.label :sheet %><br>
			    <%= f.text_field :sheet %>
			  </div>
			  <div class="field">
			    <%= f.label :note %><br>
			    <%= f.text_area :note %>
			  </div>
			  <div class="actions">
			    <%= f.submit %>
			  </div>
			<% end %>
		</div>
	</div>
</div>
